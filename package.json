{
  "name": "vnerve-docs",
  "version": "0.1.0",
  "description": "Docs for vNerve.",
  "main": "index.js",
  "scripts": {
    "dev": "vuepress dev src",
    "build": "vuepress build src",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore . --fix",
    "commit": "git cz"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^2.0.2",
    "@nuxtjs/eslint-module": "^1.1.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-vue": "^6.1.2",
    "git-cz": "^3.2.1",
    "husky": "^3.0.9",
    "lint-staged": "^9.3.0",
    "prettier": "^1.18.2",
    "vuepress": "^1.3.1"
  },
  "lint-staged": {
    "*.{js,vue}": "eslint --ext .js,.vue --fix"
  },
  "private": true,
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/git-cz"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dd-center/vNerve-Docs.git"
  },
  "keywords": [
    "vnerve"
  ],
  "author": "Il Harper",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dd-center/vNerve-Docs/issues"
  },
  "homepage": "https://github.com/dd-center/vNerve-Docs#readme"
}
